{
	"abilities": {
        "type": "array",
        "items": {
            "$ref": "abilitySchema.json"
        },
        "minItems": 1,
        "maxItems": 3,
        "additionalItems": false
  },
	"abilityOperator": {
		"type": "string",
		"enum": ["GAIN","LOSE"]
	},
	"effects": {
        "type": "array",
        "items": {
    	    "$ref": "effectSchema.json#/effect"
        },
        "minItems": 1,
        "maxItems": 4,
        "additionalItems": false
    },
	"int": {
		"type": "integer",
		"minimum": 0,
		"maximum": 100
	},
	"intChangeMode": {
		"type": "string",
		"enum": ["INCREASES", "DECREASES", "CHANGES"]
	},
	"owner": {
		"type": "string",
		"enum": ["YOUR", "OPPONENTS", "OWNERS"]
	},
	"string": {
		"type": "string",
		"minLength": 1,
		"maxLength": 32
	},
	"stringChangeMode": {
		"type": "string",
		"enum": ["CHANGES"]
	}
}